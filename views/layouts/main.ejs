    <script>
        // Function to update time and date
        function updateTimeAndDate() {
            // Get current time
            var currentTime = new Date();
            var hours = currentTime.getHours();
            var minutes = currentTime.getMinutes();
            var seconds = currentTime.getSeconds();

            // Add leading zeros if necessary
            hours = (hours < 10 ? "0" : "") + hours;
            minutes = (minutes < 10 ? "0" : "") + minutes;
            seconds = (seconds < 10 ? "0" : "") + seconds;

            // Format the time
            var timeString = hours + ":" + minutes + ":" + seconds;

            // Display time
            document.getElementById("time").textContent = "Time: " + timeString;

            // Get current date
            var currentDate = currentTime.toDateString();

            // Display date
            document.getElementById("date").textContent = "Today is " + currentDate;
        }

        // Call updateTimeAndDate function initially to display time and date
        updateTimeAndDate();

        // Update time and date every second
        setInterval(updateTimeAndDate, 1000);
    </script>

